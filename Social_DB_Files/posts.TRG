triggers='CREATE DEFINER=`root`@`localhost` TRIGGER `log_insert_posts` AFTER INSERT ON `posts` FOR EACH ROW BEGIN\n	INSERT INTO logging(log_type_id, log_type, on_table, log_context, log_time) VALUES (1, \'INSERT\', \'posts\', CONCAT(\'a new row is inserted in posts table called: \', NEW.id), CURRENT_TIMESTAMP);\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER log_update_posts AFTER UPDATE ON posts FOR EACH ROW \nBEGIN\n	INSERT INTO logging(log_type_id, log_type, on_table, log_context, log_time) VALUES (2, \'UPDATE\', \'posts\', CONCAT(\'a row is updated in posts table called: \', NEW.id), CURRENT_TIMESTAMP);\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER `history_post_update` AFTER UPDATE ON `posts` FOR EACH ROW BEGIN\n	INSERT INTO posts_history(posts_history.context, posts_history.user_id, posts_history.created_at) VALUES (OLD.context, OLD.user_id, CURRENT_TIMESTAMP);\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER log_delete_posts BEFORE DELETE ON posts FOR EACH ROW \nBEGIN\n	INSERT INTO logging(log_type_id, log_type, on_table, log_context, log_time) VALUES (3, \'DELETE\', \'posts\', CONCAT(\'a row is deleted in posts table called: \', OLD.id), CURRENT_TIMESTAMP);\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER `history_post_delete` BEFORE DELETE ON `posts` FOR EACH ROW BEGIN\n	INSERT INTO posts_history(posts_history.context, posts_history.user_id, posts_history.created_at) VALUES (OLD.context, OLD.user_id, CURRENT_TIMESTAMP);\nEND'
